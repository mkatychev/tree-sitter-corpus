{
  "name": "corpus",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "test"
      }
    },
    "test": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_header"
        },
        {
          "type": "SYMBOL",
          "name": "input"
        },
        {
          "type": "SYMBOL",
          "name": "_median"
        },
        {
          "type": "SYMBOL",
          "name": "output"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_test_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_marker": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ":skip"
            },
            {
              "type": "STRING",
              "value": ":error"
            },
            {
              "type": "STRING",
              "value": ":fail-fast"
            },
            {
              "type": "SYMBOL",
              "name": "_language_marker"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ":platform"
                },
                {
                  "type": "STRING",
                  "value": "("
                },
                {
                  "type": "SYMBOL",
                  "name": "platform"
                },
                {
                  "type": "STRING",
                  "value": ")"
                }
              ]
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_language_marker": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":language"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "language"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "language": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][\\w-]*"
    },
    "platform": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "_header_delim": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "===+"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_header_delim"
        },
        {
          "type": "SYMBOL",
          "name": "_test_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_marker"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_header_delim"
        }
      ]
    },
    "_median": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "---+"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_suffix": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^=]\\S*"
      }
    },
    "_header_delim_suffix": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "===+"
        },
        {
          "type": "SYMBOL",
          "name": "_suffix"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_header_suffix": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_header_delim_suffix"
        },
        {
          "type": "SYMBOL",
          "name": "_test_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_marker"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_header_delim_suffix"
        }
      ]
    },
    "_median_suffix": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "---+"
        },
        {
          "type": "SYMBOL",
          "name": "_suffix"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_input_line": {
      "type": "PATTERN",
      "value": ".+"
    },
    "input": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_input_line"
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "output": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "node"
      }
    },
    "node": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "ALIAS",
                        "content": {
                          "type": "SYMBOL",
                          "name": "identifier"
                        },
                        "named": true,
                        "value": "field_name"
                      },
                      {
                        "type": "STRING",
                        "value": ":"
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "node"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z_]\\w*"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": ";.*"
        }
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_newline"
    },
    {
      "type": "SYMBOL",
      "name": "text"
    }
  ],
  "inline": [],
  "supertypes": []
}
